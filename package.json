{
  "name": "introduction-to-advanced-sass",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "npm run build:styles",
    "build": "react-scripts build",
    "build:examples": "sass --style=expanded './src/examples/examples.scss' './src/examples/examples.css'",
    "test": "react-scripts test",
    "lint:scripts": "eslint \"src/**/*{ts,tsx,js,jsx}\"",
    "lint:styles": "stylelint \"src/**/*.{scss,css}\"",
    "lint": "concurrently --raw \"npm run lint:scripts\" \"npm run lint:styles\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "engines": {
    "node": "^16.6.1",
    "npm": "^8.3.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "sass": "^1.49.0",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4",
    "webpack": "^5.67.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.12",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "concurrently": "^7.0.0",
    "stylelint": "^14.3.0",
    "stylelint-config-syzygy-bem": "^2.0.0",
    "stylelint-config-syzygy-order": "^2.0.1",
    "stylelint-config-syzygy-scss": "^2.0.0",
    "stylelint-scss": "^4.1.0",
    "stylelint-selector-bem-pattern": "^2.1.1"
  }
}
